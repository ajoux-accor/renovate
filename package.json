{
  "name": "@renovate/debug-groupall",
  "version": "21.10.0-RC.4",
  "branch": "21-10-0-release",
  "description": "debug renovate groupall",
  "author": "Debug",
  "files": [
    "dist/*",
    "!dist/manifest.json"
  ],
  "scripts": {
    "i18n:download": "node ./tools/google/index.js",
    "dev:mocked": "cross-env DATA_SOURCE=in-memory LANG=fr NODE_ENV=development webpack serve --inline --progress --config config/webpack.conf.js",
    "dev": "cross-env LANG=fr NODE_ENV=development webpack serve --inline --progress --config config/webpack.conf.js",
    "start:build:mocked:for-cypress-headless": "rimraf dist/ && npm run build:for-cypress:headless && cd dist/ && http-server --silent",
    "start:dev:mocked": "rimraf dist/ && npm run dev:mocked",
    "start:dev:e2e": "rimraf dist/ && npm run dev",
    "cypress:open": "cypress open --config-file tests/cypress/cypress.json",
    "pretest:unit": "rimraf tests/unit/coverage",
    "test:unit": "jest --showConfig && cross-env VUE_APP_I18N_LOCALE=fr jest --config tests/unit/jest.conf.js --coverage --runInBand",
    "test:cypress:headless:run": "cypress run --browser chrome --headless --config-file tests/cypress/cypress-headless.json",
    "pretest:cypress:headless": "rimraf tests/cypress/coverage",
    "test:cypress:headless": "cross-env DEBUG=start-server-and-test START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start:build:mocked:for-cypress-headless http://localhost:8081/index.fr.shtml test:cypress:headless:run",
    "pretest": "npm run lint",
    "test": "npm run test:unit",
    "lint:eslint:js": "eslint --ext .js,.vue src tests/unit",
    "lint:stylelint:css": "stylelint ./src/**/*.{vue,scss}",
    "lint": "npm run lint:eslint:js && npm run lint:stylelint:css",
    "build": "rimraf dist/ && cross-env NODE_ENV=production node --no-warnings tools/build/build.js",
    "build:for-cypress:headless": "rimraf dist/ && cross-env NODE_ENV=production DATA_SOURCE=in-memory BUILD_FOR_TEST=test node --no-warnings tools/build/build.js",
    "postbuild": "node --no-warnings tools/build/generate-build-txt.js",
    "postinstall": "husky install config/husky",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "precoverage:mkdir:reports": "rimraf tests/coverage/lcov-report && rimraf tests/coverage/lcov.info && rimraf tests/reports",
    "coverage:mkdir:reports": "shx mkdir tests/reports",
    "precoverage:copy:reports": "npm run coverage:mkdir:reports",
    "coverage:copy:reports": "shx cp tests/cypress/coverage/coverage-final.json tests/reports/from-cypress.json && shx cp tests/unit/coverage/coverage-final.json tests/reports/from-jest.json",
    "precoverage:combine:reports": "npm run coverage:copy:reports",
    "coverage:combine:reports": "npx nyc merge tests/reports && shx mv coverage.json .nyc_output/out.json",
    "precoverage:report:combined": "npm run coverage:combine:reports",
    "coverage:report:combined": "npx nyc report --reporter lcov --reporter text --report-dir tests/coverage"
  },
  "dependencies": {
    "@messageformat/core": "^3.0.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "url-parse": "^1.5.3",
    "v-calendar": "^2.3.2",
    "v-mask": "^2.2.4",
    "vue": "^2.6.14",
    "vue-i18n": "^8.25.0",
    "vue-router": "^3.5.2",
    "vuejs-logger": "1.5.5",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@cypress/code-coverage": "^3.9.10",
    "@popperjs/core": "^2.9.3",
    "@vue/component-compiler-utils": "^3.2.2",
    "@vue/test-utils": "^1.2.2",
    "autoprefixer": "^10.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "chalk": "^4.1.2",
    "core-js": "^3.16.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cypress": "^8.2.0",
    "dot-object": "^2.1.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.16.0",
    "file-loader": "^6.2.0",
    "googleapis": "^85.0.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^13.0.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "lodash.clonedeep": "^4.5.0",
    "mini-css-extract-plugin": "^2.2.0",
    "ora": "^6.0.0",
    "pinst": "^2.1.6",
    "rimraf": "^3.0.2",
    "sass": "^1.37.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.1.0",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "start-server-and-test": "^1.13.1",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.20.1",
    "url-loader": "^4.1.1",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.50.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0",
    "webpack-manifest-plugin": "^4.0.2"
  },
  "engines": {
    "node": ">= 10.15.3",
    "npm": ">= 6.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "i18n": {
    "sheetTabs": [
      "search",
      "fields",
      "common"
    ],
    "supportedLanguages": [
      "ar",
